config_version=5

[application]

config/name="Sir Fallen"
config/version="1.5.5"
run/main_scene="res://menus/neverknow_splash.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="SirFallen"
config/features=PackedStringArray("4.2", "Forward Plus")
run/max_fps=60
config/icon="res://avatar.png"

[autoload]

Utils="*res://singletons/utils.gd"
Events="*res://singletons/Events.gd"
Sounds="*res://singletons/sounds.tscn"
Stats="*res://singletons/stats.gd"
GlobalSteam="*res://singletons/steam.tscn"
SaveAndLoad="*res://save_and_load/save_and_load.gd"
GlobalTimer="*res://singletons/global_timer.tscn"

[debug]

gdscript/warnings/integer_division=0
shapes/collision/shape_color=Color(1, 1, 1, 0.419608)

[display]

